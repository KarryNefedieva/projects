alias gl='git log --pretty=format:"%h %ad | %s%d [%an]" --graph  --date=short  --max-count=40 $*'
alias gs='git status'
alias ga='git add .'
alias gco='git commit -m $*'
alias gcom='git commit -a -m $*'
alias push='git push -u origin master'
alias pull='git pull'

alias pro='cd ~/new0project'

alias i='npm i $*'
alias s='npm start $*'
alias r='npm run $*'